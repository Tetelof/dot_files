{
    "files.autoSave": "afterDelay",
    "editor.fontSize": 14,
    "editor.lineHeight": 24,
    "editor.fontFamily": "Fira Code",
    "editor.wordWrap": "on",
    "editor.minimap.enabled": true,
    "editor.minimap.scale": 1,
    "editor.renderWhitespace": "boundary",
    "editor.cursorBlinking": "solid",
    "editor.fontLigatures": true,
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.iconTheme": "vs-seti",
    "workbench.colorCustomizations": {
        "editor.background": "#000000",
        "editor.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#00ffff11",
        "editor.selectionBackground": "#00ffff44",
        "editorCursor.foreground": "#ffffff",
        "editorWhitespace.foreground": "#444444",
        "editorIndentGuide.background": "#444444",
        "editorIndentGuide.activeBackground": "#444444",
        "editorLineNumber.foreground": "#444444",
        "editorLineNumber.activeForeground": "#00ff00",
        "activityBar.background": "#000000",
        "badge.background": "#000000",
        "menu.background": "#000000",
        "input.background": "#000000",
        "panel.background": "#000000",
        "banner.background": "#000000",
        "button.background": "#000000",
        "minimap.background": "#000000",
        "sideBar.background": "#000000",
        "terminal.background": "#000000",
        "statusBar.background": "#000000"
    },
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "Consolas, 'Courier New', monospace",
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.cursorBlinking": true,
    "editor.tokenColorCustomizations": {
        "strings": "#ff009d",
        "comments": "#00ff00",
        "variables": "#04ff00",
        "types": "#FF0000",
        "functions": "#FF0000",
        "keywords": "#00ffff",
        "numbers": "#00ffff",
        "textMateRules": []
    },
    "window.zoomLevel": -1,
    "workbench.startupEditor": "none",
    "git.autofetch": true,
    "editor.inlineSuggest.enabled": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.formatOnPaste": true,
    "editor.minimap.size": "proportional"
}
